<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="attachmentMapper">
	<insert id="insertAttachment">
		INSERT
		  INTO
		  		ATTACHMENT
		  		VALUES
		  		(
		  		SEQ_ANO.NEXTVAL
		  	   ,#{originName}
		  	   ,#{changeName}
		  	   ,null
		  	   ,SYSDATE
		  	   ,#{boardNo}
		  	   ,#{boardType}
		  		)
		  		
	</insert>
</mapper>
